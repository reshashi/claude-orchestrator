{
  "name": "claude-orchestrator",
  "version": "3.1.0",
  "description": "Cross-platform Claude Code worker orchestration with background processes and HTTP API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "claude-orchestrator": "./bin/claude-orchestrator"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "serve": "node dist/index.js serve",
    "lint": "eslint src/**/*.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rm -rf dist"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "fastify": "^4.26.0",
    "@fastify/websocket": "^10.0.0",
    "@fastify/cors": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "claude",
    "ai",
    "orchestrator",
    "automation",
    "moltbot",
    "parallel-workers"
  ],
  "author": "",
  "license": "MIT"
}
